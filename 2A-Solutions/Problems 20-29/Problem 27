//439A - Devu, the Singer and Churu, the Joker (C++)

#include<bits/stdc++.h>
#define ll long long int
#define HACKS std::ios::sync_with_stdio(false);cin.tie(NULL);
#define pb push_back
#define mp make_pair
#define fi first
#define se second
using namespace std;

int main() 
{ 
  HACKS

  int n,d;
  cin >> n >> d;
  
  int sum = 0;
  
  for(int i=0;i<n;i++){
    int a;
    cin >> a;
    sum += a;
  }
  
  if((sum+(n-1)*10) <= d){
    int ans = (d-sum)/5;
    cout << ans;
  }else{
    cout << "-1";
  }
  
  return 0; 
  
  
} 

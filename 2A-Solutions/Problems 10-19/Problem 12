// 451A - Game With Sticks (C++)
#include<bits/stdc++.h>
#define ll long long int
#define IO std::ios::sync_with_stdio(false);cin.tie(NULL);
#define pb push_back
#define mp make_pair

using namespace std;

int main() {
  IO
   
  int n=0,m=0;
  cin >> n >> m;
  
  int move = 0;
  if(n<m){
    move = n;
  }else
    {
      move = m;
    }
    
  if(move%2==0){
    cout << "Malvika" << endl;
  }  else{
    cout << "Akshat" << endl;
  }
  
}

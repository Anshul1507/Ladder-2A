//263A - Beautiful Matrix (C++)

#include<bits/stdc++.h>
#define ll long long int
#define IO std::ios::sync_with_stdio(false);cin.tie(NULL);
#define pb push_back
#define mp make_pair

using namespace std;

int main() {
    IO
   
  int arr[4][4];
  int x=0,y=0;
  for(int i=0;i<=4;i++){
    for(int j=0;j<=4;j++){
      int n;
      cin >> n;
      if(n == 1){
        x = i;
        y = j;
        continue;
      }
    }
  }
  int res = abs(2-x) + abs(2-y);
  cout << res;
  
}

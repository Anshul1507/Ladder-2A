//501A - Contest (C++)

#include<bits/stdc++.h>
#define ll unsigned long long int
#define HACKS std::ios::sync_with_stdio(false);cin.tie(NULL);
#define pb push_back
#define mp make_pair
#define fi first
#define se second
using namespace std;

int main() 
{ 
  HACKS
  int a,b,c,d;
  
  cin >> a >> b >> c >> d;
  
  ll ans1 = max( (3*a)/10 , (a - (a*c)/250) );
  ll ans2 = max( ((3*b)/10) , (b - (b*d)/250) );
  
  if(ans1 > ans2){
    cout << "Misha";
  }else if(ans1 < ans2){
    cout << "Vasya";
  }else{
    cout << "Tie";
  }
  return 0; 
  
} 
